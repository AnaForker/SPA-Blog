{
  "parser": "babel-eslint",
  "extends": "af-react",
  "rules": {
    "jsx-a11y/href-no-hash": 0,
    "arrow-parens": 0,
    "global-require": 0,
    "import/prefer-default-export": 0,
    "no-console": 0,
    "no-mixed-operators": 0,
    "no-use-before-define": 0,
    "radix": 0,
    "react/jsx-filename-extension": 0,
    "react/require-default-props": 0,
    "react/forbid-prop-types": 0,
    "react/no-array-index-key": 0,
    "react/jsx-no-bind": 0,
    "react/prop-types": 0,
    "react/sort-comp": 0,
    "react/no-multi-comp": 0,
    "semi": 0,
    "linebreak-style": 0,
    "arrow-body-style": 0,
    "one-var": 0,
    "no-underscore-dangle": 0,
    "no-plusplus": 0,
    "no-param-reassign": 0,
    "no-shadow": 0,
    "no-else-return": 0,
    "camelcase": 0,
    "prefer-const": 1,
    "guard-for-in": 0,
    "no-restricted-syntax": 0,
    "no-sequences": 0,
    "class-methods-use-this": 0,
    "no-unused-expressions": 0,
    "no-useless-escape": 0,
    "no-cond-assign": 0,
    "no-undef": 0,
  }
}
